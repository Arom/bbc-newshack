{% include 'header.html.twig' %}
<div class="container" id="top-boxes">
    <div id="weather-box">
        <div class="weather-left">
            <img id="weatherIcon" src="" class="weather-icon">
            <img id="windIcon" src="" class="weather-icon">
        </div>
        <div class="weather-right">
            <p id="hour" class="weather-hour-location"></p>
            <p id="temp" class="weather-temperature"></p>
            <p id="windSpeed" class="weather-wind-speed"></p>
            <p id="visibility" class="weather-visibility"></p>
        </div>
    </div>
    <div id="events-box">
        <span class="event-title"></span>
        <span class="event-image"></span>
    </div>
</div>
